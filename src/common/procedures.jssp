
[parameterType="SpNoticeListParam", resultType="NoticeListResult"]
proc spNoticeList(out ret integer) (NoticeVo[]) {
  select * from notice;
  ret = Success();
  return;
}

[parameterType="SpGetUserInfoParam"]
proc spGetUserInfo(out ret integer, _userId UserID) UserVo {
  select userId, nickname, email from user
  where userId = _userId;
  ret = Success();
  return;
}

    
query insertDelivery(deliveryStatus Type, memberId NumID) Long {

	insert into delivery(
		delivery_status, member_id
	) values (
		:deliveryStatus, :memberId
	)

}

query updateDelivery(deliveryId NumID, deliveryStatus Type) Long {

	update delivery set
		delivery_status = :deliveryStatus
	where delivery_id = :deliveryId
}

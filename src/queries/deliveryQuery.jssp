query insertDelivery(deliveryStatus Type, memberId NumID) {

	insert into delivery(
		delivery_status, member_id
	) values (
		:delveryStatus, memberId
	) RETURNING delivery_id

}

query updateDelivery(deliveryId NumID, deliveryStatus Type) {

	update delivery set
		delivery_status = :deliveryStatus
	where delivery_id = :deliveryId
	RETURNING delivery_id
}

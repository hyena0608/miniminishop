query insertMember(memberType TypeID, memberName Nickname, memberPhone Phone) Long {

	insert into member (
		member_type, member_name, member_phone
	) values (
		:memberType, :memberName, :memberPhone
	)

}

query selectMemberList() MemberVo[] {

	select * from member

}

query selectMemberById(memberId Long) MemberVo {
	select * from member
	where member_id = :memberId
}

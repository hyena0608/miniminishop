query insertOrder(orderStatus Type, memberId Long, deliveryId Long, riderId Long) Long {

	insert into orders(
		order_status, member_id, delivery_id, riderId
	) values (
		:orderStatus, :memberId, :deliveryId, :riderId
	)

}

query updateOrderStatus(orderId NumID, orderStatus Type) Long {

	update orders set
		order_status = :orderStatus
	where
		order_id = :orderId

}


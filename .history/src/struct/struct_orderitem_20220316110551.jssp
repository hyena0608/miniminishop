struct Orderitem '주문_상품'{

  orderitem_id Long;

  memberId Long;
  orderId Long;
  itemId Long;
  itemPrice Money;
  itemAmount Amount;

}

struct ItemOrdered '주문된 상품 정보' {

    itemId Long;
    itemName Name;
    itemAmount Amount;
    itemPrice Money;

}

struct OrderitemDetail '주문_상품리스트' {

    orderId Long;
    orderStatus Type;
    deliveryStatus Type;
    itemsOrdered list<ItemOrdered>;

}